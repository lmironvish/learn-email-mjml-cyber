<mj-attributes>

  <!-- global attr mjml -->

  <mj-all                                    font-family="Helvetica, Arial, sans-serif"
                                             font-size="20px"
                                             line-height="25px"
                                             font-weight="400"
                                             font-style="normal"
                                             color="#000000" />

  <mj-class name="h1"                        font-size="34px"
                                             line-height="40px"
                                             font-weight="700" />

  <mj-class name="h2"                        font-size="28px"
                                             line-height="35px"
                                             font-weight="700" />

  <mj-class name="h3"                        font-size="16px"
                                             line-height="22px"
                                             font-weight="700" />

  <mj-class name="small"                     font-size="14px"
                                             line-height="20px"
                                             color="#838383" />

  <mj-wrapper                                padding="0px"
                                             full-width="full-width"/>

  <mj-section                                padding="0px" />

  <mj-image                                  padding="0px"
                                             rel="noopener noreferrer" />

  <mj-button                                 padding="0px"
                                             rel="noopener noreferrer"
                                             target="_blank" />

  <mj-text                                   align="center"/>

  <mj-class name="divider"                   padding="0px"
                                             padding-bottom="20px"
                                             border-width="1px"
                                             border-style="solid"
                                             border-color="#838383" />

  <mj-class name="divider-spacer"            padding="10px 0px"
                                             border-width="0px"
                                             border-style="solid"
                                             border-color="#ffffff" />

  <mj-class name="container"                 width="87%" />

  <!-- helpers -->

  <!-- padding-bottom -->

  <mj-class name="pb0"                       padding-bottom="0px" />
  <mj-class name="pb10"                      padding-bottom="10px" />
  <mj-class name="pb15"                      padding-bottom="15px" />
  <mj-class name="pb20"                      padding-bottom="20px" />
  <mj-class name="pb25"                      padding-bottom="25px" />
  <mj-class name="pb30"                      padding-bottom="30px" />
  <mj-class name="pb40"                      padding-bottom="40px" />
  <mj-class name="pb50"                      padding-bottom="50px" />
  <mj-class name="pb60"                      padding-bottom="60px" />

  <!-- padding-top -->

  <mj-class name="pt0"                       padding-top="0px" />
  <mj-class name="pt10"                      padding-top="10px" />
  <mj-class name="pt15"                      padding-top="15px" />
  <mj-class name="pt20"                      padding-top="20px" />
  <mj-class name="pt25"                      padding-top="25px" />
  <mj-class name="pt30"                      padding-top="30px" />
  <mj-class name="pt40"                      padding-top="40px" />
  <mj-class name="pt50"                      padding-top="50px" />
  <mj-class name="pt60"                      padding-top="60px" />

  <!-- blocks -->

  <mj-class name="wrapper"                   background-url="https://i.postimg.cc/WbpmfjSn/img-cyber-main-bg.jpg"
                                             background-position="left top"
                                             background-repeat="no-repeat"
                                             background-size="40%"
                                             width="100%"/>

  <!-- title -->

  <mj-class name="views-title__logo"         width="450px"
                                             padding="10px 20px"/>

  <!-- header -->

  <mj-class name="views-header"              background-color="#0e0e10"/>

  <mj-class name="views-header__navbar"      hamburger="hamburger"
                                             ico-close="✖"
                                             ico-open="⚌"
                                             ico-color="#a114ba"/>

  <mj-class name="views-header__navlink"     href="#"
                                             font-family="Montserrat, Courier, sans-serif"
                                             font-size="16px"
                                             font-weight="normal"
                                             text-decoration="none"
                                             text-transform="uppercase"
                                             line-height="24px"
                                             color="#bbbbbb"
                                             padding="10px 20px"/>

  <mj-class name="views-header__img"         padding="10px 0px"/>

  <!-- adv -->
  <mj-class name="views-adv"                 padding="0px 10px"
                                             padding-top="10px"
                                             background-color="#0e0e10"/>

  <mj-class name="views-adv__column"         padding="0px 10px"
                                             padding-bottom="10px"/>

  <mj-class name="views-adv__accord"         border="0px"
                                             icon-wrapped-url="https://i.postimg.cc/XNfxzsWV/img-cyber-down-arrow.png"
                                             icon-unwrapped-url="https://i.postimg.cc/PJXDczbx/img-cyber-up-arrow.png"
                                             icon-width="14px"
                                             padding="0px"/>

  <mj-class name="views-adv__accord-title"   font-family="Montserrat, Courier, sans-serif"
                                             font-size="12px"
                                             color="#bbbbbb"
                                             padding="10px"/>

  <mj-class name="views-adv__accord-text"    font-family="Roboto, Arial, sans-serif"
                                             font-size="14px"
                                             line-height="22px"
                                             color="#bbbbbb"
                                             padding="10px"/>

  <mj-class name="views-adv__img"            width="64px"
                                             padding="10px"/>

  <!-- galery -->

  <mj-class name="rtl"                       direction="rtl"/>

  <mj-class name="views-gallery"             padding="10px"
                                             background-color="#0e0e10"/>

  <mj-class name="views-gallery__img-top"    width="190px"
                                             padding="10px"/>

  <mj-class name="views-gallery__img-bottom" width="250px"
                                             padding="10px"/>

  <mj-class name="views-gallery__img-blink"  width="270px"
                                             padding="10px"/>

  <mj-class name="views-gallery__caption"    font-family="Montserrat, Courier, sans-serif"
                                             font-size="16px"
                                             text-align="left"
                                             align="left"
                                             text-transform="uppercase"
                                             line-height="24px"
                                             padding="10px"
                                             color="#bbbbbb"/>

  <mj-class name="views-gallery__text"       font-family="Roboto, Arial, sans-serif"
                                             font-size="14px"
                                             text-align="left"
                                             align="left"
                                             line-height="22px"
                                             padding="10px"
                                             color="#bbbbbb"/>

  <mj-class name="views-gallery__button"     href="#"
                                             background-color="#a114ba"
                                             color="#bbbbbb"
                                             font-family="Roboto, Arial, sans-serif"
                                             font-size="14px"
                                             font-weight="normal"
                                             line-height="22px"
                                             padding="10px"
                                             border-radius="6px"/>

  <!-- carousel -->
  <mj-class name="views-carousel"            background-color="#0e0e10"
                                             padding="20px"/>

  <mj-class name="views-carousel__wrap"      left-icon="https://i.postimg.cc/JhH5cqKC/img-cyber-left-arrow.png"
                                             right-icon="https://i.postimg.cc/T2Jq9hj1/img-cyber-right-arrow.png"
                                             icon-width="20px"
                                             tb-width="50px"
                                             tb-selected-border-color="#a114ba"
                                             tb-border-radius="6px"
                                             tb-border="2px solid transparent"
                                             tb-hover-border-color="#a114ba"/>

  <!-- footer -->

  <mj-class name="views-footer"              background-color="#0e0e10"/>

  <mj-class name="views-footer__img"         width="50px"
                                             padding="10px"/>

  <mj-class name="views-footer__link"        font-family="Montserrat, Courier, sans-serif"
                                             font-size="12px"
                                             text-align="left"
                                             align="left"
                                             text-transform="uppercase"
                                             line-height="20px"
                                             text-decoration="none"
                                             padding="10px"
                                             color="#bbbbbb"/>

  <mj-class name="views-footer__caption"     font-family="Montserrat, Courier, sans-serif"
                                             font-size="16px"
                                             text-align="left"
                                             align="left"
                                             text-transform="uppercase"
                                             line-height="24px"
                                             padding="10px"
                                             color="#bbbbbb"/>

  <mj-class name="views-footer__text"        font-family="Roboto, Arial, sans-serif"
                                             font-size="14px"
                                             text-align="left"
                                             align="left"
                                             line-height="22px"
                                             padding="10px"
                                             color="#bbbbbb"/>

  <mj-class name="views-footer__copyright"   font-family="Roboto, Arial, sans-serif"
                                             font-size="14px"
                                             text-align="left"
                                             align="left"
                                             line-height="22px"
                                             padding="10px"
                                             color="#bbbbbb"/>

  <mj-class name="views-footer__social"      icon-width="20px"
                                             padding="10px"/>


</mj-attributes>
